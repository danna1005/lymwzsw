<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.thiscc.xtsz.entity">
    
    <class name="UserInfo" table="t_userInfo" dynamic-insert="true" dynamic-update="true">
        <cache usage="nonstrict-read-write"/>
        <id name="id" type="java.lang.Integer" unsaved-value="null">
            <column name="id" length="4"/>
            <generator class="native"/>
        </id>
        
        
        <property name="userno" column="userno" />
        <property name="userpass" column="userpass" />
        <property name="username" column="username" />
        <property name="userphone" column="userphone" />
        <property name="remark" column="remark" />
        
    </class>
</hibernate-mapping>