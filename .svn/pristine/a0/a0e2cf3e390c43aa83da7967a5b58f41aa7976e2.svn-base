/*document files name -- animate*/
@charset "utf-8";

/*旋转*/
.rotate-360{transform: rotate(360deg);-webkit-transform: rotate(360deg);-moz-transform: rotate(360deg);}
/*water wave*/
.swaView-Wavebx{ position:absolute; width:17px; height:17px; border-radius:50%;background-color:rgba(255,255,255,0.06);}
.swaView-WaveIns{ position:absolute; left:8px; top:8px; width:1px; height:1px; border-radius:50%;background-color:rgba(255,255,255,0.12);}

/*anminate*/
.animate-fillboth{
	animation-fill-mode:both;
	-webkit-animation-fill-mode:both;
}
.defaultExample{
	animation-name:defaultExample;
	-webkit-animation-name:defaultExample;
	animation-duration:0.7s;
	-webkit-animation-duration:0.7s;
	animation-timing-function:cubic-bezier(0.45,1,0.25,1);
	-webkit-animation-timing-function:cubic-bezier(0.45,1,0.25,1);
}

@-webkit-keyframes defaultExample{
	0%{ -webkit-transform:translate3d(100%,0,120px); opacity:0;}
	80%{ -webkit-transform:translate3d(-20,0,60px);}
	100%{ -webkit-transform:translate3d(0,0,0); opacity:1}
}
@keyframes defaultExample{
	0%{ transform:translate3d(100%,0,120px); opacity:0;}
	80%{ transform:translate3d(-20,0,60px);}
	100%{ transform:translate3d(0,0,0); opacity:1}
}

/*水波纹*/
.animate-wave{
	animation-name:wave;
	-webkit-animation-name:wave;
	animation-duration:0.3s;
	-webkit-animation-duration:0.3s;
	animation-timing-function:cubic-bezier(0.25,1,0.45,1);
	-webkit-animation-timing-function:cubic-bezier(0.25,1,0.45,1);
}
@-webkit-keyframes wave{
	0%{ -webkit-transform:scale(1); opacity:1;}
	100%{ -webkit-transform:scale(2); opacity:0;}
}
@keyframes wave{
	0%{ transform:scale(1); opacity:1;}
	100%{ transform:scale(2); opacity:0;}
}
.animate-waveIns{
	animation-name:waveIns;
	-webkit-animation-name:waveIns;
	animation-duration:0.5s;
	-webkit-animation-duration:0.5s;
	animation-delay:0.2;
	-webkit-animation-delay:0.2;
	animation-timing-function:cubic-bezier(0.65,1,0.45,1);
	-webkit-animation-timing-function:cubic-bezier(0.65,1,0.45,1);
}
@-webkit-keyframes waveIns{
	0%{ -webkit-transform:scale(1); opacity:1;}
	100%{ -webkit-transform:scale(8); opacity:0;}
}
@keyframes waveIns{
	0%{ transform:scale(1); opacity:1;}
	100%{ transform:scale(8); opacity:0;}
}





