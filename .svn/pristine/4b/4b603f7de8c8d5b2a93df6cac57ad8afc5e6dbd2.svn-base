@charset "utf-8";
/* CSS Document */
*{ margin:0px; padding:0px;}
.clear{ margin:0px; padding:0px; clear:both;}
.clear-fix { *overflow: hidden; *zoom: 1; }  
.clear-fix:after { display: table; content: ""; width: 0; clear: both; }  

html,body,div,dl,dt,dd,ul,h1,h2,h3,pre,form,label,fieldset,p,blockquote,table,input,textarea{font-size:14px;margin:0;padding:0; font-family:"微软雅黑",Arial,Helvetica, sans-serif;} 
body{ margin:0 auto; text-align:center; background:#f2f2f2;}
fieldset,img{border:0}
code,em,var,i,b{font-style:normal;}
ul{list-style:none}
li {list-style-type: none;}
.clearfloat{height:0;font-size:1px;clear:both;line-height:0;}
a{text-decoration:none; color:#000;}
.inputer,.txtarea{ border:0; line-height: 30px;}
.txtarea{border:0;resize:none; height: 100px;}
.photo-icon{display: inline-block; background: url(../images/general/photoicon.png) 0 0 no-repeat; width: 30px; height: 30px; background-size: 30px 30px;}
.clear{clear:both;}
.input-btn{ 
	background: #1DACF4; 
	border:0;margin: 0 10px; 
	color:#fff; height:45px;display: inline-block; 
	text-align: center; font-size: 16px;
	width:40%;
	vertical-align: top;
	line-height: 45px;
}
.s-title{white-space: nowrap;overflow: hidden;text-overflow:ellipsis;width:70%}
.s-detail-header{background:#0e90d2;height: 50px; line-height: 50px;}
.s-detail-header .header-title{width: 75%; text-overflow:ellipsis; white-space: nowrap; overflow: hidden;}
.clor-cont{color:#666;}
.s-h-img{max-width: 90%; text-align: center; margin:15px auto 0; display: block;}
.slider{ position: relative; width: 100%; height: 180px; overflow: hidden;}
.slider ul{ position: absolute; z-index: 2; height: 180px;}
.slider li{ height: 180px; width: 100%; float: left;}
.slider li img{ width:100%; height:180px;}
.category{float: left; position: relative;margin:0 0 5px 2%; width: 96%; overflow: hidden;}
.nav {overflow: hidden; width: 96%; padding:2%;}
.nav li{float:left; width: 21%; margin:1% 2%;} 
.nav li a{width: 100%;}
.nav li .rancle{text-align:center;border: 3px solid #fff; box-shadow: 0px 1px 2px rgba(0, 0, 0, 0.10); border-radius: 50%; overflow: hidden; width: 60px; height: 60px; margin: 0 auto;}
.nav li .rancle img{width: 100%; height: 100%; vertical-align: middle;}
.nav li .tit{color:#666; width: 100%; line-height: 28px;margin-top: 10%; overflow: hidden; white-space: nowrap; text-overflow:ellipsis; font-size:14px;}
.bg-light-gray{background: #f8f8f8;}
.img100width{width: 100%;}
.radisT{border-top-left-radius: 6px; border-top-right-radius: 6px}
.cname{
    font-size: 16px;text-align: left; font-weight: normal; line-height: 40px; color:#fff; text-indent: 10px;
    background: #0083c7;
    background: -ms-linear-gradient(top, #86c6df,  #1c6aaa);        /* IE 10 */
    background:-moz-linear-gradient(top,#86c6df,#1c6aaa);/*火狐*/ 
    background:-webkit-gradient(linear, 0% 0%, 0% 100%,from(#86c6df), to(#1c6aaa));/*谷歌*/
    background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#86c6df), to(#1c6aaa));      /* Safari 4-5, Chrome 1-9*/
    background: -webkit-linear-gradient(top, #86c6df, #1c6aaa);   /*Safari5.1 Chrome 10+*/
    background: -o-linear-gradient(top, #86c6df, #1c6aaa);  /*Opera 11.10+*/
}
.temporary{overflow: hidden;}
.temporary p{width: 33.333%; float: left;}
.temporary a img{width: 100%;}
.temporary p .panel{width:80%; margin:0 auto;}
.temporary p .panel img{width:60%; display:block;margin:10px auto; border-radius:6px;}
.temporary p .panel em{color:#666; display:block; width:100%; text-align:center; margin:5px auto;font-size:14px;}
.thumb_btn{overflow: hidden; position: absolute; bottom: 10px;left:50%; z-index: 3; margin-left: -30px;}
.thumb_btn span{ float:left; text-indent: -1000em; background-color: #fff; width: 10px; height: 10px; border-radius: 50%; display: block; margin-right: 8px; z-index: 3}
.article{ line-height: 22px; color:#555; text-align: left; padding:10px 20px;}
.article p{font-size: 14px;}
.txt-indent{text-indent: 30px}
.h2-cls{font-size: 14px; line-height: 24px; color:#444; margin-top:10px;}
.marT-50{margin-top: 50px;}
.feedback-list{overflow: hidden; font-size: 12px; line-height: 30px; text-align: left;}
.feedback-list li{overflow: hidden; border-bottom: 1px dotted #dedede; padding: 10px 0;}
h3.f-title{line-height: 30px; color: #00a0ea}
.f-answer{color: #777;}
.f-date{color: #888;}
.news-list{overflow: hidden;height: 410px;}
.news-list li{ overflow: hidden; line-height: 30px; position: relative; }
.news-tit{padding-right: 80px; display: block; white-space: nowrap; overflow: hidden; text-overflow:ellipsis;}
.news-date{color:#888; position: absolute; right: 0;top:6px;}
.detail-tit{font-weight: normal;margin:10px 0; text-align: center;}
.detail-date{line-height: 25px; text-align: center; border-bottom: 1px dashed #e4e4e4; padding-bottom: 5px;}
.detail-cont p{font-size: 14px;}
.detail-cont img{max-width: 100%;}
.empty{text-align: center; padding: 50px 0; font-size: 14px;}
.page {
    float: right;
    height: 56px;
    padding: 26px 0 0;
    text-align: center;
}
.page a {
    background-color: #eeeeee;
    color: #666666;
    float: left;
    line-height: 20px;
    margin: 0 3px 0 0;
    padding: 4px 12px 3px;
    text-decoration: none;
}
.page span {
    float: left;
    line-height: 20px;
    margin: 0 3px 0 0;
    padding: 4px 12px 0;
}
.page a:hover, .page .selected {
    background-color: #3777b4;
    color: #ffffff;
}
.page .current {
    background-color: #3777b4;
    color: #ffffff;
    height: 23px;
}
.page span.next, span.prev {
    background: rgba(0, 0, 0, 0) none repeat scroll 0 0 !important;
    color: #666666 !important;
}



 