* {list-style-type: none; margin: 0; padding: 0;}
body{ margin:0px; padding:0px; font-family:"宋体";background:#C6D9F2 url(../images/main_bg.jpg) center top repeat-x; }
a{ text-decoration:none; outline:none}
ul{ margin:0px; padding:0px; list-style-type: none;}
a img{list-style-type: none; border:0 none;}
.mb5{ margin-bottom:5px;}
.fl{ float:left}
.fr{ float:right;}
.clear{ clear:both;}
.bggry{ background-color:#f6fafc;}
.bolder{ font-weight:bolder}
#top{ height:79px; line-height:79px; margin:auto; background: url(../images/top_bg.jpg) repeat-x;}
.w980{ margin:0 auto; width:980px;}
#top .logo{ float:left; padding-top:5px;}
.logo img{float:left;}
#top .nav{ float:right;}
#top .nav a{ width:91px; float:left; color:#255EA1; font-size:14px; vertical-align:middle; text-align:center; height:79px;}
#top .nav a.selected,#top .nav a:hover{ background:url(../images/header_bg.jpg) no-repeat; color:#FFF;}

#cont{ background:url(../images/banner_dt.jpg) no-repeat center top; height:359px; padding-top: 20px;}
#cont .banner{ z-index:1}
.posiup{position: relative; top: -25px}
.contboxbg{ z-index:2;background:url(../images/main_cont_bg.png) no-repeat center top; height:260px;overflow:hidden; padding:30px 35px 0 35px ; margin-top: 20px;}
.contboxbg0{ z-index:2;background:#FFF; height:260px;overflow:hidden; padding:30px 35px 0 35px ;}
#contbox .title{ background:url(../images/main_line_title.png) no-repeat; height:30px; font-size:14px;} 
#contbox .title a{ color:#3777b4; text-decoration:none;}
#contbox .title a:hover{ color:#2cb2fb; text-decoration:none;}
#contbox .title .more a:hover{ color:#3777b4; text-decoration:underline;}

#contbox .news{float:left; width:280px; margin-right:40px;}
#contbox .news ul.newslist{ font-size:12px; padding:7px 0 0 0; line-height:27px; }
#contbox .news ul.newslist a{ color:#999999; text-decoration:none; width:216px; height:100%; overflow:hidden; display:inline-block;}
#contbox .news ul.newslist a:hover{ color:#3777b4; text-decoration:underline;}
#contbox .news ul.newslist li{color:#999999; padding:0; height:27px;}
#contbox .news ul.newslist span{display:inline-block;}
#contbox .news .cont{color:#999;font-size: 12px; padding: 10px 0; line-height: 20px;}
#contbox .notify{float:left;width:280px; }
#contbox .notify ul.newslist{ font-size:12px; padding:7px 0 0 0; line-height:22px;}
#contbox .notify ul.newslist a{ color:#3777b4;text-decoration:none; display:inline-block; width:100%; overflow:hidden}
#contbox .notify ul.newslist a:hover{ color:#3777b4;text-decoration:underline;}
#contbox .notify ul.newslist li{ _line-height:25px; padding:2px 0 2px 0; color:#999999; border-bottom:#eaeaea 1px dotted; height:49px;}
*+html #contbox .notify ul.newslist li { line-height:24px; } /* IE7 */
#contbox .notify ul.newslist li .time{ font-size:12px; font-weight:400px; color:#777777; display:inline-block}

#contbox .serve{float:right;}
#contbox .serve ul{}
#contbox .serve ul li{ line-height:22px; height:58px; margin-bottom:0;margin-bottom:6px!important;}
#contbox .picbox{background:#FFF; height:auto; padding:30px 35px 0 35px ;}
#contbox .picbox .title{ background:url(../images/main_line_newspic.png) no-repeat; height:30px; font-size:14px;} 

/*picture rotation */
.mr_frbox{height:140px;padding:24px 0 0 0;}
.mr_frBtnL{float:left;margin:28px 22px 0px 0px;cursor:pointer;display:inline;}
.mr_frUl{float:left;height:130px;width:840px;}
.mr_frBtnR{float:right;margin-top:28px;cursor:pointer;}
.mr_frbox .mr_frUl ul li{display:inline;float:left;margin-right:13px;overflow:hidden;}
#contbox .picbox .mr_frbox .mr_frUl ul li img{display:block;margin-bottom:7px;border:5px solid #f0f0f0;}
#contbox .picbox .mr_frbox .mr_frUl ul li a:hover img{border:5px solid #A0C0EB;}

/*footer*/
#footer{background-color:#323e4e; height:63px; color:#c3c8ce; position:relative;bottom:-20px;}
#footer .logo_white{float:left; padding-top:11px; margin-right:10px;}
#footer span{font-size:12px; float:left; margin:10px 0 10px 10px; padding:10px 0 10px 10px;display:inline-block;}
#footer .phone{font-size:12px; margin:10px 0 10px 0; padding:10px  0 10px 0; float:right;}

/*secondary*/
#secondary{padding:10px 0 0 0;}
#secondary .bigbox{ margin:10px 0 0 0; border:#d8d9dd 1px solid; background-color:#FFF; width:980px; height:auto; display:block;}
#secondary .left{background:url(../images/left_ds_03.jpg) no-repeat scroll right top rgba(0, 0, 0, 0); display: inline; float: left;  min-height:537px;height:auto;margin:20px 0 0 20px; width:205px;}
#secondary  .left .left_bt{background:url(../images/left_t1_06.jpg) no-repeat center bottom;color: #0063a1;height: 50px;width: 176px;}
.left_bt p {color: #666;font-size: 14px; height: 20px; line-height: 20px;  margin: 0; padding: 0;font-family:"Microsoft YaHei";}
.left_bt p span {color: #0063a1; font-size: 14px; height: 20px;line-height: 20px;font-family:"Microsoft YaHei";}
.left_nav {font-family:"Microsoft YaHei"; margin-top: 5px; overflow: auto; width: 176px;}
.left_nav ul li {background:url(../images/left_icon_li01.jpg) no-repeat scroll 9% 50% rgba(0, 0, 0, 0);border-bottom: 1px solid #ccc;color: #666;font-size: 14px; height: 39px;line-height: 39px; padding-left: 30px; width: 146px;}
.left_nav ul li a { color: #666; font-size: 14px;}
.left_nav ul li a:hover { color:#00a0ea; font-size: 14px;}
#blue { background:url(../images/left_2_sel.jpg) no-repeat  left center; color: #fff; font-size: 14px;height: 39px; width: 146px;}
#blue2,.nav-change li.cur{ background:url(../images/left_2_sel_0.jpg) no-repeat left center; color: #fff; font-size: 14px;height: 39px; width: 146px;}
#blue a,#blue2 a,.nav-change li.cur a,.nav-change li.cur a:hover { color: #fff; font-size: 14px;}
.map {background:url(../images/left_bg_water.jpg) no-repeat scroll 0 0 rgba(0, 0, 0, 0); height: 190px; margin-top: 20px; width: 190px;}

#secondary .bigbox .right {display: inline; float: right; margin-right: 30px;margin-top: 20px; width: 695px;}
#secondary .right .cont_title {border-bottom: 1px dotted #00a0ea; height: 40px; width: 690px;}
.cont_title_posi {color: #666;  float: left; font-size: 16px; height: 40px; line-height: 40px;}
.cont_title_posi  a{color: #00a0ea; font-size: 12px;}
.cont_title_posi  a:hover{color: #00a0ea; text-decoration:underline;}
.cont_title_posi  span {color: #666;  font-size: 12px;}
.right_contact { background:url(../images/serve.jpg) no-repeat right center ; height: 291px; margin-top: 50px; width: 690px;}
.right_contact h1 {color: #00a0ea;font-size: 14px;line-height: 20px;}
.right_contact ul { color: #666; font-size: 14px; line-height: 24px; margin:10px 0 0 0; padding: 0;}
.right_contact ul span { color: #00a0ea;  font-size: 12px;  line-height: 24px;}

.severmap{width:697px;height:550px;border:#ccc solid 1px; margin:20px 0 20px 0;}
.iw_poi_title {color:#CC5522;font-size:14px;font-weight:bold;overflow:hidden;padding-right:13px;white-space:nowrap}
.iw_poi_content {font:12px arial,sans-serif;overflow:visible;padding-top:4px;white-space:-moz-pre-wrap;word-wrap:break-word}

.cont_news_list{color: #666; font-size: 12px; margin: 20px 0; overflow: auto; width: 690px;}
.cont_news_list ul li {background:url(../images/l2.gif) no-repeat scroll left center rgba(0, 0, 0, 0); border-bottom: 1px dotted #ccc; color: #666;   font-size: 14px; height: 40px;line-height: 40px; padding-left: 20px; width: 670px;}
.cont_news_list ul li:hover{ background-color:#f6fafc;}
.cont_news_list ul li a { color: #666; font-size: 14px;}
.cont_news_list ul li a:hover { color: #0063a1; text-decoration:underline;}
.cont_news_list ul li em { float: right; font-style: normal; margin-right:10px;}

.page {background-color: #ffffff; height: 56px; padding:26px 0 0 0; text-align: center; float:right;}
.page a {background-color: #eeeeee; color: #666666; float: left; line-height: 20px; margin: 0 3px 0 0; padding: 4px 12px 3px;text-decoration: none;}
.page span {float: left; line-height: 20px; margin: 0 3px 0 0; padding: 4px 12px 0;}
.page a:hover, .page .selected {background-color: #3777b4;  color: #ffffff;}
.page .current {background-color: #3777b4;  color: #ffffff; height:23px;}
.page span.next, span.prev{background:none !important;  color: #666666 !important;}

.cont_content{color: #666; font-size: 14px; margin: 20px 0; overflow: auto; width: 690px;}
.cont_content h1{ text-align:center; margin:0 0 15px 0;font-family: "Microsoft YaHei"; font-size:20px; font-weight:500;}
.cont_content p{ line-height:22px;}

.txt{ font-size:12px; line-height:22px; color:#777777;}




.navbox{ float:right;position:relative; height:50px; z-index:999;}
.navbox table{_margin-bottom:-4px}
.navbox table tr td{padding:0px; vertical-align:top}
.navbox .menu_dropdownbg{ background-color:#fff; FILTER: alpha(opacity=95);/*box-shadow:0px 5px 8px #a9a9a9; -moz-box-shadow:0px 5px 8px #a9a9a9;-webkit-box-shadow:0px 5px 8px #a9a9a9;*/
                          -webkit-box-shadow:0 5px 10px rgba(0, 0, 0, 0.3); 
                          -moz-box-shadow:0 5px 10px rgba(0, 0, 0, 0.3);  
                          box-shadow:0 5px 10px rgba(0, 0, 0, 0.3);
                          filter: progid:DXImageTransform.Microsoft.Shadow(Strength=3, Direction=90, Color='#d1d1d1');
                          -ms-filter: "progid:DXImageTransform.Microsoft.Shadow(Strength=3, Direction=90, Color='#d1d1d1')"; 
                          overflow:hidden; z-index:3; position:absolute;top:79px; margin-left:0px; margin-top:0px; line-height:24px; 
                        }
.menu_dropdownsplit{ background:url(../images/menudropsplit.png) no-repeat; height:2px;  overflow:hidden; position:absolute; left:0px; top:-1px;}
.navbox .menu_dropdownbg ul{background:url(../images/cc_03.png) repeat-x; padding:10px; position:relative; _display:block;}
.navbox .menu_dropdownbg ul li{ display:block; _width:121px;}
.navbox .menu_dropdownbg ul li a{ color:#666; display:block;white-space:nowrap; min-width:95px; font-size: 12px; text-align: center}
.navbox .menu_dropdownbg ul li a:hover,.navbox .menu_dropdownbg li a.select{ color:#666; display:block;color:#c00;}
/******************菜单********************************/
.logo-name{color:#255EA1; font-size: 24px; line-height: 70px; float: left;}
.navTab a.nav{ width:115px; height:79px; display:block; margin:0 auto; line-height: 79px; text-align: center; color: #0063a1;font-size: 16px;}
.navTab a.nav:hover,.navTab a.nav.selected{font-weight: bold;color:#fff;background:url(../images/header_bg.jpg) repeat-x;}
.txt-indent{text-indent: 30px;}
.padT-20{padding-top: 20px;}
.content-ul>li{display: none;}
.content-ul>li.select{display: block;}
.contact-tab{line-height: 30px; color:#666; font-size: 12px;}
.contact-tab th{font-weight: normal; text-align: left;}
.help-info li{position: relative; overflow: hidden; margin-bottom: 20px;}
.help-info h1{font-size: 16px; color:#555; padding:5px 10px; text-align: left;}
.help-info-view{color:#666;margin:0 auto; position: relative; width: 94%;border:1px solid #999; padding:10px; float: left; font-size: 12px;}
.j-img{ background: url(../images/arrow.png) no-repeat; width: 13px; height: 7px; overflow: hidden; position: absolute;left:25px; top:-7px;}
.clearfix{clear: both; display: block;}
.help-info .margin-l10{margin-left:10px; width:95%;}
.h2-cls{font-size: 16px;}
.feedback-list{overflow: hidden; font-size: 12px; line-height: 30px;}
.feedback-list li{overflow: hidden; border-bottom: 1px dotted #dedede; padding: 10px 0;}
h3.f-title{line-height: 30px; color: #00a0ea}
.f-answer{color: #777;}
.f-date{color: #888;}
.outlet-tab{font-size: 12px;}
.outlet-tab td{padding: 5px}
.pubTime{color:#888;font-size: 14px; text-align: center; margin-bottom: 20px; padding-bottom: 15px; border-bottom: 1px solid #eaeaea; font-weight: normal;}
.fd-form{line-height: 30px;color:#666;font-size: 12px; margin:20px;}
.form-cell{margin-bottom: 10px; overflow: hidden;}
.form-cell::after{content: '';clear: both;}
.field{float:left; margin-right: 10px; width: 80px; text-align: right;}
.field em{color:#c00; font-style: normal;}
.fd-form input[type="text"],.fd-form textarea{ padding: 0 5px; border:1px solid #eaeaea; border-radius: 2px; height: 30px; line-height: 30px; width: 300px; color:#777;float:left;}
.fd-form textarea{height: 100px; resize: none; font-size: 14px;}
.fd-form .submit{background: #037FBB;cursor:pointer; border-radius: 5px; height: 35px; width: 80px; line-height: 35px; color:#fff;margin: 0 auto;margin-left:90px; border:0;}
.uploadify{float:left; position: relative;}
.upload-btn{background: #f8f8f8; border:1px solid #eaeaea;border-radius: 2px; color:#888; width: 90px; height: 32px; text-align: center;}
.uploadify-queue{ position: absolute;left:90px; top:35px; z-index: 2; display: none}
.file-img{max-width: 500px;}.ke-upload-area{overflow:visible;}
.ind-banner{overflow: hidden;}
.focus{width: 970px; overflow: hidden; height: 330px; position: relative; border:5px solid #DBEAF9;}
.focus ul{height: 290px; position: absolute;}
.focus ul li{width: 970px;margin:0 auto; float: left; height: 330px; text-align: center; position: relative; overflow: hidden;}
.focus ul li img{width: 970px; height: 330px;}
.thumb_btn{ position: absolute; right: 0; bottom: 10px; z-index: 2}
.thumb_btn span{background:#fff; display: block;float: left; width: 60px; height: 45px; border:3px solid #fff;margin-right: 5px;}

.fd-form .validate{color:#c00; line-height: 30px; margin-left: 5px; float: left;}
.slt-type{width:300px;padding:0 5px; margin-left:90px; position:relative;border:1px solid #eaeaea; cursor:pointer;}
.slt-type .down{z-index:2; display:block; background:url(../images/down.png) 0 0 no-repeat; width:14px; height:12px; position:absolute;bottom:8px; right:5px; cursor:pointer;}
.slt-a{width:280px;line-height:30px;}
.slt-b{background:#fff;position:absolute; top:31px;left:-1px; z-index:2; border:1px solid #eaeaea; border-top:0; width:310px;}
.slt-b li{ display:block; line-height:30px; cursor:pointer;padding:0 5px;}
.slt-b li:hover{background:#f2f2f2;}
.slt-panel{margin-bottom:10px;}
.slt-panel:after{content:''; width:0; clear:both;}
.hide{display:none;}